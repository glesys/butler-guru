{
    "name": "glesys/butler-guru",
    "description": "An experimental and opinionated RabbitMQ package for laravel/lumen.",
    "type": "library",
    "authors": [
        {
            "name": "Emil Andersson",
            "email": "emil@glesys.se"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Butler\\Guru\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Guru\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Butler\\Guru\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/bus": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0",
        "bschmitt/laravel-amqp": "^1.2 || ^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5|^9.0",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
